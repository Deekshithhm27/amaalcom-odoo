<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record id="seq_payment_approval" model="ir.sequence">
            <field name="name">Payment Approval</field>
            <field name="code">account.payment.approval</field>
            <field name="prefix">PA/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_draft_invoice" model="ir.sequence">
            <field name="name">Draft Invoice</field>
            <field name="code">draft.account.move</field>
            <field name="prefix">D-Inv/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_draft_invoice" model="ir.sequence">
            <field name="name">Draft Invoice Sequence</field>
            <field name="code">account.move.draft.invoice</field>
            <field name="prefix">DRAFT/INV/</field> <!-- Customize the prefix as needed -->
            <field name="padding">5</field> <!-- Customize the padding as needed -->
            <field name="company_id" eval="False"/>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>