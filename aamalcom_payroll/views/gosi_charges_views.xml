<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <record id="gosi_charges_tree" model="ir.ui.view">
        <field name="name">Gosi Charges</field>
        <field name="model">gosi.charges</field>
        <field name="arch" type="xml">
            <tree string="Gosi Charges" create="0" editable="top">
