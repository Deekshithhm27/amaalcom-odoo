<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
    	<record id="seq_visa_candidate" model="ir.sequence">
            <field name="name">Candidate</field>
            <field name="code">visa.candidate</field>
            <field name="prefix">CLIENT_SEQUENCE</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_service_request" model="ir.sequence">
            <field name="name">Service Request</field>
            <field name="code">service.request</field>
            <field name="prefix">SERVICE_REQUEST_SEQUENCE</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_local_transfer" model="ir.sequence">
            <field name="name">Local Transfer</field>
            <field name="code">local.transfer</field>
            <field name="prefix">LT</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_employment_visa" model="ir.sequence">
            <field name="name">Employment Visa</field>
            <field name="code">employment.visa</field>
            <field name="prefix">EV</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_service_request_approval" model="ir.sequence">
            <field name="name">Service Request Approval</field>
            <field name="code">service.request.approval</field>
            <field name="prefix">SRA</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>